{
  "$schema": "http://json-schema.org/schema#",
  "additionalProperties": {
    "properties": {
      "app_id": {
        "type": "string"
      },
      "notification_emails": {
        "type": "array",
        "items": {
          "type": "string",
          "format": "email"
        }
      },
      "url": {
        "type": "string",
        "format": "uri"
      },
      "metrics_files": {
        "type": "array",
        "items": {
          "type": "string",
          "pattern": "metrics\\.yaml$"
        }
      },
      "dependencies_format": {
        "type": "string",
        "enum": [
          "gradle"
        ]
      },
      "dependencies": {
        "type": "array",
        "item": {
          "type": "string"
        }
      },
      "library_names": {
        "type": "array",
        "item": {
          "type": "string"
        }
      }
    },
    "required": [
      "app_id",
      "url",
      "notification_emails"
    ],
    "type": "object"
  },
  "type": "object"
}
